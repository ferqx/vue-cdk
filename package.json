    }
  },
  "dependencies": {
    "node-sass": "^6.0.1",
    "sass": "^1.44.0",
    "vscode-css-languageservice": "^5.1.8",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@vitejs/plugin-vue": "^1.9.3",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  }
}
