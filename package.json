{
  "name": "vue-cdk",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/my-lib.umd.js",
